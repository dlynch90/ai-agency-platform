{
  "workflow_id": "healthcare-triage",
  "description": "AI symptom checker and appointment prioritization",
  "steps": [
    {
      "name": "symptom_analysis",
      "type": "ollama_inference",
      "model": "llama3.2:3b",
      "prompt": "Analyze patient symptoms and medical history",
      "inputs": ["symptoms", "patient_history"],
      "outputs": ["triage_assessment"]
    },
    {
      "name": "urgency_classification",
      "type": "ml_inference", 
      "model": "medical_classifier",
      "inputs": ["triage_assessment", "medical_knowledge"],
      "outputs": ["priority_level", "recommended_action"]
    },
    {
      "name": "appointment_scheduling",
      "type": "database_operation",
      "database": "postgresql",
      "operation": "schedule_appointment",
      "inputs": ["priority_level", "availability"],
      "outputs": ["appointment_confirmed"]
    }
  ],
  "triggers": ["symptom_submission", "emergency_alert"],
  "integrations": ["postgresql", "ollama", "redis", "neo4j"]
}
