{
  "name": "ai-agency-platform",
  "version": "1.0.0",
  "description": "Enterprise AI Agency Platform with Multi-Client Support",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "test:e2e": "turbo run test:e2e",
    "test:tdd": "vitest --config vitest.config.tdd.ts --watch",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "validate": "turbo run validate",
    "validate:vendor": "turbo run validate:vendor",
    "db:generate": "prisma generate --schema=database/prisma/schema.prisma",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "graphql:codegen": "graphql-code-generator",
    "evaluate": "turbo run evaluate",
    "evaluate:promptfoo": "promptfoo eval --config configs/promptfoo/promptfooconfig.yaml",
    "evaluate:deepeval": "python -m deepeval test run tests/evaluation/",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "infrastructure:up": "./bin/infrastructure-up",
    "infrastructure:down": "docker compose -f infra/docker-compose.development.yml down",
    "infrastructure:logs": "docker compose -f infra/docker-compose.development.yml logs -f",
    "mcp:health": "curl -s http://localhost:11434/api/tags && echo ' Ollama OK'",
    "health": "npx tsx tools/health-analyzer/src/cli.ts --path .",
    "health:ultrathink": "npx tsx tools/health-analyzer/src/cli.ts --ultrathink --path .",
    "health:json": "npx tsx tools/health-analyzer/src/cli.ts --ultrathink --path . --format json",
    "health:markdown": "npx tsx tools/health-analyzer/src/cli.ts --ultrathink --path . --format markdown",
    "health:api": "npx tsx tools/health-analyzer/src/api/server.ts",
    "health:fix": "npx tsx tools/health-analyzer/src/cli.ts fix --path .",
    "health:fix:dry-run": "npx tsx tools/health-analyzer/src/cli.ts fix --path . --dry-run",
    "health:fix:critical": "npx tsx tools/health-analyzer/src/cli.ts fix --path . --severity critical,high",
    "health:report": "npx tsx tools/health-analyzer/src/cli.ts --ultrathink --path . --format markdown --output docs/health-report.md",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@1password/sdk": "^0.3.1",
    "@apollo/gateway": "^2.12.2",
    "@apollo/server": "^5.2.0",
    "@apollo/subgraph": "^2.12.2",
    "@clerk/clerk-sdk-node": "^4.13.14",
    "@fastify/postgres": "^5.2.2",
    "@hono/zod-validator": "^0.7.6",
    "@langchain/community": "^1.1.1",
    "@langchain/core": "^1.1.8",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/mcp-adapters": "^1.1.1",
    "@langchain/openai": "^1.2.0",
    "@prisma/client": "^5.7.1",
    "apollo-server": "^3.13.0",
    "graphql": "^16.8.1",
    "hono": "^4.0.0",
    "redis": "^4.6.12",
    "vitest": "^1.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ama-mcp/github": "^13.4.2",
    "@danielsimonjr/memory-mcp": "^0.48.0",
    "@gofman3/task-master-mcp": "^2.2.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@henrychong-ai/mcp-neo4j-knowledge-graph": "^1.8.6",
    "@modelcontextprotocol/inspector": "^0.18.0",
    "@modelcontextprotocol/server-filesystem": "^2025.12.18",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@modelcontextprotocol/server-postgres": "^0.6.2",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.12.18",
    "@playwright/mcp": "^0.0.53",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.56.0",
    "express": "^5.2.1",
    "jest-environment-jsdom": "^30.2.0",
    "lefthook": "^1.5.0",
    "neo4j-driver": "^6.0.1",
    "prettier": "^3.2.0",
    "prisma": "^5.7.1",
    "prom-client": "^15.1.3",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "keywords": [
    "ai-agency",
    "multi-tenant",
    "enterprise",
    "graphql",
    "microservices",
    "event-driven"
  ],
  "author": "AI Agency Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-agency-platform"
  }
}
