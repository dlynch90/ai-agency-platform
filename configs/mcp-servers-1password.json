{
  "$schema": "https://modelcontextprotocol.io/schema/mcp-config.json",
  "version": "2.0.0",
  "description": "MCP Server Configuration with 1Password Secret Injection",
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${DEVELOPER_DIR:-$HOME/Developer}"
      ],
      "env": {
        "MCP_FILESYSTEM_ROOT": "${DEVELOPER_DIR:-$HOME/Developer}"
      }
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@danielsimonjr/memory-mcp"
      ],
      "env": {
        "MEMORY_DB_PATH": "${DEVELOPER_DIR:-$HOME/Developer}/data/memory.db"
      }
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {
        "SEQUENTIAL_THINKING_DB_PATH": "${DEVELOPER_DIR:-$HOME/Developer}/data/sequential-thinking.db"
      }
    },
    "ollama": {
      "command": "node",
      "args": [
        "mcp-servers/ollama-server.cjs"
      ],
      "env": {
        "OLLAMA_HOST": "http://localhost:11434"
      }
    },
    "openai": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-openai"
      ],
      "env": {
        "OPENAI_API_KEY": "op://MCP Servers/OpenAI MCP Server/api_key"
      }
    },
    "anthropic": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-anthropic"
      ],
      "env": {
        "ANTHROPIC_API_KEY": "op://MCP Servers/Anthropic MCP Server/api_key"
      }
    },
    "huggingface": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-huggingface"
      ],
      "env": {
        "HUGGINGFACE_API_KEY": "op://MCP Servers/HuggingFace MCP Server/api_key"
      }
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "op://MCP Servers/Brave Search MCP Server/api_key"
      }
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp"
      ],
      "env": {
        "TAVILY_API_KEY": "op://MCP Servers/Tavily MCP Server/api_key"
      }
    },
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "exa-mcp-server"
      ],
      "env": {
        "EXA_API_KEY": "op://MCP Servers/Exa MCP Server/api_key"
      }
    },
    "firecrawl": {
      "command": "npx",
      "args": [
        "-y",
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "op://MCP Servers/Firecrawl MCP Server/api_key"
      }
    },
    "deepwiki": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-deepwiki"
      ],
      "env": {}
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres"
      ],
      "env": {
        "DATABASE_URL": "op://MCP Servers/PostgreSQL MCP Server/connection_string"
      }
    },
    "redis": {
      "command": "npx",
      "args": [
        "-y",
        "redis-mcp"
      ],
      "env": {
        "REDIS_URL": "redis://localhost:6379"
      }
    },
    "neo4j": {
      "command": "npx",
      "args": [
        "-y",
        "@henrychong-ai/mcp-neo4j-knowledge-graph"
      ],
      "env": {
        "NEO4J_CONNECTION_URL": "op://MCP Servers/Neo4j MCP Server/url",
        "NEO4J_PASSWORD": "op://MCP Servers/Neo4j MCP Server/password"
      }
    },
    "qdrant": {
      "command": "npx",
      "args": [
        "-y",
        "qdrant-api-mcp"
      ],
      "env": {
        "QDRANT_API_KEY": "op://MCP Servers/Qdrant MCP Server/api_key",
        "QDRANT_CLUSTER_URL": "op://MCP Servers/Qdrant MCP Server/url"
