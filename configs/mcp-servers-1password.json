    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@ama-mcp/github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "op://MCP Servers/GitHub MCP Server/token"
