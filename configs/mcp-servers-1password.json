    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@anthropic/mcp-server-git"
      ],
      "env": {
        "GIT_DIR": "/Users/daniellynch/Developer/.git"
      }
    },
    "docker": {
      "command": "npx",
      "args": [
        "-y",
        "docker-mcp"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      }
    },
    "kubernetes": {
      "command": "npx",
      "args": [
        "-y",
        "kubernetes-mcp"
      ],
      "env": {
        "KUBECONFIG": "${HOME}/.kube/config"
