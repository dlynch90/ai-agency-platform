        "N8N_API_KEY": "op://MCP Servers/N8N MCP Server/api_key"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp"
      ],
      "env": {
        "HEADLESS": "true"
      }
    },
    "gibson": {
      "command": "gibson",
      "args": [
        "mcp",
        "run"
      ],
      "env": {
        "GIBSONAI_PROJECT": "4b3c4aef-9704-4ca3-abb8-669ad2e0f0c4",
        "PATH": "/Users/daniellynch/.local/bin:$PATH"
      }
