    "notion": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-notion"
      ],
      "env": {
        "NOTION_TOKEN": "op://MCP Servers/Notion MCP Server/token"
      }
    },
    "linear": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-linear"
      ],
      "env": {
        "LINEAR_API_KEY": "op://MCP Servers/Linear MCP Server/api_key"
      }
    },
    "task-master": {
      "command": "npx",
      "args": [
        "-y",
        "@gofman3/task-master-mcp"
      ],
      "env": {
        "TASK_DB_PATH": "${HOME}/.claude/tasks.db"
