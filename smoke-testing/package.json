{
  "name": "ai-agency-smoke-testing",
  "version": "1.0.0",
  "description": "Comprehensive smoke testing proxy for AI Agency applications",
  "main": "proxy-server.js",
  "scripts": {
    "start": "node proxy-server.js",
    "dev": "nodemon proxy-server.js",
    "test": "npm run test-smoke",
    "test-smoke": "node -e \"require('./proxy-server.js'); // CONSOLE_LOG_VIOLATION: console.log('Proxy loaded for testing')\"",
    "health": "curl -s http://localhost:8080/smoke/health | jq .",
    "full-test": "curl -s http://localhost:8080/smoke/test-all | jq ."
  },
  "dependencies": {
    "express": "^4.18.0",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "keywords": ["testing", "proxy", "smoke-tests", "ai-agency"],
  "author": "AI Agency",
  "license": "MIT"
}