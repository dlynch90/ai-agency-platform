{
  "workflow_id": "ecommerce-personalization",
  "description": "AI-powered product recommendations and dynamic pricing",
  "steps": [
    {
      "name": "user_analysis",
      "type": "ollama_inference",
      "model": "llama3.2:3b",
      "prompt": "Analyze user browsing history and preferences",
      "inputs": ["user_history", "purchase_data"],
      "outputs": ["user_profile"]
    },
    {
      "name": "product_matching", 
      "type": "vector_search",
      "database": "qdrant",
      "inputs": ["user_profile", "product_catalog"],
      "outputs": ["recommendations"]
    },
    {
      "name": "pricing_optimization",
      "type": "ml_inference",
      "model": "pricing_model",
      "inputs": ["recommendations", "market_data"],
      "outputs": ["dynamic_pricing"]
    }
  ],
  "triggers": ["user_login", "cart_abandonment", "browsing_session"],
  "integrations": ["postgresql", "redis", "neo4j", "ollama"]
}
